#include<bits/stdc++.h>

using namespace std;
  
int main(){
  map<int, int> a;
  a.insert(pair<int, int>(1, 40));
  //where 1 is key which is unique and 40 is the element
  
  
  
  //for printing:--------------------------------------
  map<int, int>::iterator itr;
  for (itr = a.begin(); itr != a.end(); ++itr) {
        cout << '\t' << itr->first                               //key
             << '\t' << itr->second << '\n';                     //element
    }
    
    
    
    //assigning to the second map----------------
    map<int, int> b(a.begin(), a.end());
    
    
    //---------------------removing-----------------
    a.erase(KEY);  
    b.erase(b.begin(), b.find(KEY));
    
    //OTHER
    a.find(KEY); //position of key
    a.count(KEY); //if key is present or no(bool)
    a.size(); //size of element
    a.empty(); //if a is empty(bool)
    a.clear(); //clears
    a.rbegin() = a.end();
    a.emplace(FIRST, SECOND);
}
